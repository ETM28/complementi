
cout<<hello,pablito calvo;


#include <iostream>
using namespace std;

int main(int argc, char **argv)
{
	//la funzione sarà y=i+5
	double i,a,b;

	cout<<"inserisci l'intervallo: \nA=";
	cin>>a;
	cout<<"B=";
	cin>>b;

	for(i=a;i<b;i=i++)
	{
		if(i+5==0)
		{
			cout<<"la funzione è 0 in "<<i<<endl;
		}
	}


	return 0;
}
